{
  "name": "modern-backbone-starterkit",
  "version": "1.0.0",
  "description": "Lightweight starting point for a modern backbone web app.",
  "keywords": [
    "backbone",
    "starterkit",
    "frontend",
    "boilerplate",
    "es6",
    "webpack"
  ],
  "scripts": {
    "start": "serve --port 3000 -s public",
    "test": "jest",
    "dev": "node webpack/webpack-dev-server",
    "coverage": "yarn test -- --coverage",
    "lint": "eslint src",
    "build-prod": "cross-env NODE_ENV=production webpack --progress --color --config webpack/webpack.config",
    "build-alpha": "cross-env NODE_ENV=alpha webpack --progress --color --config webpack/webpack.config",
    "build-beta": "cross-env NODE_ENV=beta webpack --progress --color --config webpack/webpack.config",
    "build-v": "cross-env NODE_ENV=alpha webpack --progress --color --display-error-details --config webpack/webpack.config",
    "prepush": "yarn lint"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.hbs$": "<rootDir>/webpack/jestHandlebarsLoader.js"
    }
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.16.0",
    "babili-webpack-plugin": "^0.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-airbnb-base": "^11.1.2",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.4",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.10.0",
    "file-upload": "^0.0.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "imports-loader": "^0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "^18.1.0",
    "json-loader": "^0.5.4",
    "mustache-loader": "^0.4.1",
    "nodemon": "^1.11.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.0.3",
    "serve": "^5.1.2",
    "style-loader": "^0.13.1",
    "typescript": "^2.4.2",
    "typings-for-css-modules-loader": "^1.5.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-dotenv-plugin": "^2.0.0"
  },
  "dependencies": {
    "@types/react": "^16.0.5",
    "backbone": "^1.3.3",
    "backbone.marionette": "^3.2.0",
    "flexboxgrid": "^6.3.1",
    "framework7": "^2.0.0-beta.8",
    "framework7-react": "^0.9.2-6-beta",
    "highcharts": "^5.0.12",
    "history": "^4.7.2",
    "hogan": "^1.0.2",
    "jquery": "^3.2.1",
    "material-components-web": "^0.19.0",
    "moment": "^2.18.1",
    "mustache": "^2.3.0",
    "normalize.css": "^5.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "scriptjs": "^2.5.8",
    "ts-loader": "^2.3.4",
    "underscore": "^1.8.3",
    "zeroclipboard": "^2.3.0"
  }
}
